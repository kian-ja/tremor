function [CC,F] = coherence_EMG_Torque(EGM,torque)
%%parameters
fs = 1000;
frequency = 0 : 1: fs/2;
overLap = 500;
windowLength = 1000;

num_EMG_channel = size(EMG,2);
CC = zeros(length(frequency),)
for i = 1 : num_EMG_channel
    [CC,F] = mscohere(x,y,gausswin(windowLength),overLap,frequency,fs);
end

end